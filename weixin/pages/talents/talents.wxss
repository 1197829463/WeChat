/* 弹出框样式 */


  .modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
  }
  .modal-dialog {
    width: 92%;
    position: fixed;
    top: 30%;
    left: 0;
    z-index: 9999;
    background: #ffffff;
    margin: 4%;
    border-radius: 20rpx;
    /* position: absolute; */
  }

.modal-title text {
    color: #e56a1c;
}
  .modal-title {
    width: 100%;
    height: 90rpx;
    background-color: #eee;
    text-align: center;
    line-height: 90rpx;
  }

.modal-title .text-top{
    margin: 0 30rpx;
    font-size: 26rpx;
    color: #7f7f7f;
    border-radius: 10rpx;
}
.modal-content {
    padding: 20rpx 20rpx;
}
  

/* 下拉框样式 */
.select_box{
    width:75%;
    position: relative;
    border-left:1rpx solid #cccccc;
    padding-left: 20rpx;
  }
  .select_box .select{
    box-sizing: border-box;
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
    display: flex;
    align-items: center;
    padding: 0 10rpx;
  }
  .select_box .select .select_text{
    font-size: 26rpx;
    line-height: 32rpx;
    flex: 1;
  }
  .select_box .select .select_img{
    width: 30rpx;
    height: 30rpx;
    display: block;
    transition:transform 0.3s;
  }
  .select_box .select .select_img_rotate{
    transform:rotate(180deg); 
  }
  .select_box .option_box{
    position: absolute;
    top: calc(100% - 1px);
    width: 100%;
    box-sizing: border-box;
    height: 0;
    overflow-y: auto;
    background: #fff;
    transition: height 0.3s;
    border-left:1px solid #efefef;
    border-right:1px solid #efefef;
    z-index: 999999999;
    border-radius: 10rpx;
    margin-top: 20rpx
    
  }
  .select_box .option_box .option{
    display: block;
    line-height: 40rpx; 
    font-size: 26rpx;
    border-top: 1px solid #efefef;
    border-bottom: 1px solid #efefef;
    padding: 10rpx;
    margin: 0 10rpx;
    z-index: 999999999;
  }


  .modal-btn{
    background-color: #e56a1c;
    color: #fff;
    height: 70rpx;
    line-height: 70rpx;
    margin: 20rpx 0 35rpx 0;
    font-size: 32rpx; 
  }

  .modal-footer {
    position: absolute;
    top: 110%;
    left: 50%;
    transform: translate(-50%);
    width: 50rpx;
    height: 50rpx;

  }
  .modal-footer image {
      width: 100%;
      height: 100%;
  }